{
  "name": "earthdata-pub-forms",
  "version": "1.0.15",
  "private": true,
  "description": "Forms for Earthdata Pub API",
  "author": "Earthdata Pub Dev Team",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "clean-and-instrument": "npm run instrument && npm run cp",
    "test": "cypress run",
    "preinstrument": "npm run clean && npm run remake-dirs",
    "remake-dirs": "mkdir -m 777 build || true && mkdir -m 777 build/src || true ",
    "test:unit": "jest -c jest.config.js --all",
    "instrument": "npx nyc instrument --compact false src build/src || true && cp -r src/assets/ build/src || true && npx nyc instrument --compact false src/assets build/src/assets",
    "cp": "cp .* build || true && cp *.config.js build || true && cp package.json build || true && cp -r public build || true && cp -r cypress build || true",
    "clean": "rm -rf build",
    "report:coverage": "nyc report --reporter=text-summary --exclude-after-remap false",
    "report:coverage-all": "nyc report --reporter=text-summary --reporter=html --all --require @babel/register --exclude-after-remap false",
    "finalize-coverage": "mv build/.nyc_output build/coverage && mv build/coverage . && npm run clean",
    "lint": "vue-cli-service lint",
    "build-docs": "vuese gen --include='**/(components|mixins)/*.(vue|js)'",
    "clean-modules": "rm -rf package-lock.json node_modules/",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cypress": "npx cypress open",
    "cypress-headless": "npx cypress run",
    "cypress-headless-all": "npx cypress run --browser chrome; npx cypress run --browser chromium; npx cypress run --browser edge; npx cypress run --browser electron",
    "link-api": "npm link ../api",
    "link-repos": "npm run link-api && cd ../earthdata-pub-dashboard && npm run link-api",
    "open-docs": "vuese serve --open",
    "snyk-prepare": "npm run snyk-protect",
    "snyk-protect": "snyk protect",
    "snyk-test": "snyk test",
    "start": "npm run start-api; npm run start-dashboard; npm run start-forms-dev",
    "start-api": "npm run link-api; cd ../earthdata-pub-api && npm run start:detached",
    "start-dashboard": "cd ../earthdata-pub-dashboard && npm run start",
    "start-dev": "npm run start-api; npm run start-dashboard; npm run start-forms-dev; npm run start-overview-dev",
    "start-forms": "docker-compose -f docker-compose.yml up -d",
    "start-forms-dev": "docker-compose -f docker-compose-dev.yml up -d",
    "start-overview-dev": "cd ../earthdata-pub-overview && npm run start-overview-dev",
    "stop": "npm run stop-api; npm run stop-dashboard; npm run stop-forms-dev",
    "stop-api": "cd ../earthdata-pub-api && npm run stop",
    "stop-dashboard": "cd ../earthdata-pub-dashboard && npm run stop",
    "stop-dev": "npm run stop-api; npm run stop-dashboard; npm run stop-forms-dev; npm run stop-overview-dev",
    "stop-forms": "docker-compose -f docker-compose.yml down",
    "stop-forms-dev": "docker-compose -f docker-compose-dev.yml down",
    "stop-overview-dev": "cd ../earthdata-pub-overview && npm run stop-overview-dev",
    "view-logs": "npm run start-api; docker-compose -f docker-compose.yml up"
  },
  "main": "index.js",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/vue-fontawesome": "^2.0.6",
    "@inotom/vue-go-top": "^1.3.0",
    "bootstrap": "^4.5.3",
    "bootstrap-vue": "^2.21.2",
    "bootstrap-vue-editable-table": "0.1.0-beta.9",
    "cypress-localstorage-commands": "^1.6.1",
    "dotenv": "^14.3.2",
    "js-logger": "^1.6.1",
    "jsonwebtoken": "^9.0.0",
    "vue": "^2.6.14",
    "vue-fixed-header": "^3.2.15",
    "vue-router": "^3.5.3",
    "vuejs-logger": "^1.5.5",
    "vuelidate": "^0.7.6",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/eslint-parser": "^7.16.3",
    "@babel/preset-env": "^7.23.6",
    "@cypress/code-coverage": "^3.12.15",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/test-utils": "^1.3.6",
    "@vuese/cli": "^2.1.0",
    "acorn": "^8.6.0",
    "axe-core": "^4.4.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-istanbul": "^6.1.1",
    "babel-plugin-js-logger": "^1.0.17",
    "cypress": "^13.6.1",
    "cypress-axe": "^1.5.0",
    "eslint": "^7.4.0",
    "eslint-plugin-vue": "^8.2.0",
    "eslint-webpack-plugin": "^3.1.1",
    "istanbul-lib-coverage": "^2.0.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "nyc": "^14.1.1",
    "serve": "^13.0.2",
    "vue-jest": "^3.0.7",
    "vue-template-compiler": "^2.6.14",
    "execa": "1.0.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:cypress/recommended"
    ]
  },
  "bugs": {
    "url": "https://bugs.earthdata.nasa.gov/browse/EDPUB"
  },
  "engines": {
    "node": "18.14.1"
  },
  "homepage": "https://git.earthdata.nasa.gov/projects/EDPUB/repos/forms/browse",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://git.earthdata.nasa.gov/scm/EDPUB/forms.git"
  },
  "nyc": {
    "extension": [
      ".js",
      ".vue"
    ]
  }
}