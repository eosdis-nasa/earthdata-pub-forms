{
  "name": "earthdata-pub-forms",
  "version": "1.0.0",
  "description": "Forms for Earthdata Pub API",
  "repository": {
    "type": "git",
    "url": "https://git.earthdata.nasa.gov/scm/EDPUB/forms.git"
  },
  "author": {
    "name": "Earthdata Pub Dev Team"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://bugs.earthdata.nasa.gov/browse/EDPUB"
  },
  "homepage": "https://git.earthdata.nasa.gov/projects/EDPUB/repos/forms/browse",
  "private": true,
  "engines": {
    "node": ">=12.22.0"
  },
  "scripts": {
    "clean-modules": "rm -rf package-lock.json node_modules/",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit --env=jsdom",
    "lint": "vue-cli-service lint",
    "snyk-protect": "snyk protect",
    "snyk-test": "snyk test",
    "snyk-prepare": "npm run snyk-protect",
    "link-api": "npm link ../api",
    "link-repos": "npm run link-api && cd ../dashboard && npm run link-api",
    "start-api": "npm run link-api; docker-compose -f ./node_modules/earthdata-pub-modules/docker-compose.yml up -d; ",
    "stop-api": "docker-compose -f ./node_modules/earthdata-pub-modules/docker-compose.yml down",
    "start-forms": "docker-compose -f docker-compose.yml up -d",
    "stop-forms": "docker-compose -f docker-compose.yml down",
    "start-dashboard": "cd ../dashboard && npm run start",
    "stop-dashboard": "cd ../dashboard && npm run stop",
    "start-forms-dev": "docker-compose -f docker-compose-dev.yml up -d",
    "stop-forms-dev": "docker-compose -f docker-compose-dev.yml down",
    "start": "npm run start-api; npm run start-dashboard; npm run start-forms",
    "stop": "npm run stop-api; npm run stop-forms",
    "start-dev": "npm run start-api; npm run start-forms-dev",
    "stop-dev": "npm run stop-api; npm run stop-forms-dev",
    "view-logs": "npm run start-api; docker-compose -f docker-compose.yml up"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/vue-fontawesome": "^2.0.6",
    "@inotom/vue-go-top": "^1.3.0",
    "bootstrap": "^4.5.3",
    "bootstrap-vue": "^2.21.2",
    "bootstrap-vue-editable-table": "0.1.0-beta.9",
    "jquery": "^3.6.0",
    "js-logger": "^1.6.1",
    "jsonwebtoken": "^8.5.1",
    "snyk": "^1.786.0",
    "vue": "^2.6.14",
    "vue-fixed-header": "^3.2.15",
    "vue-router": "^3.5.3",
    "vuejs-logger": "^1.5.5",
    "vuelidate": "^0.7.6",
    "vuex": "^3.6.2",
    "vuex-undo-redo": "^1.1.4"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@vue/cli-plugin-babel": "^4.5.15",
    "@vue/cli-plugin-eslint": "^5.0.0-rc.1",
    "@vue/cli-plugin-unit-jest": "^4.5.15",
    "@vue/cli-service": "^4.5.15",
    "@vue/test-utils": "^1.3.0",
    "acorn": "^8.6.0",
    "babel-jest": "^27.4.2",
    "babel-plugin-js-logger": "^1.0.17",
    "cypress": "^9.2.1",
    "eslint": "^7.4.0",
    "eslint-plugin-vue": "^8.2.0",
    "eslint-webpack-plugin": "^3.1.1",
    "jest-environment-jsdom": "^27.4.3",
    "jest-environment-jsdom-global": "^3.0.0",
    "jest-localstorage-mock": "^2.4.18",
    "jest-serializer-vue": "^2.0.2",
    "jsdom": "^19.0.0",
    "serve": "^13.0.2",
    "vue-jest": "^3.0.7",
    "vue-template-compiler": "^2.6.14"
  },
  "snyk": true
}
